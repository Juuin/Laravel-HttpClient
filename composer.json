{
    "name": "juuin/laravel-httpclient",
    "description": "HttpClient Facade in Laravel",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "HttpClient",
        "curl"
    ],
    "authors": [
        {
            "name": "Hongfei Zhang",
            "email": "angus00761@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Juuin\\HttpClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juuin\\HttpClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juuin\\HttpClient\\HttpClientServiceProvider"
            ],
            "aliases": {
                "HttpClient": "Juuin\\HttpClient\\Facades\\HttpClient"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
